{
  "apps": {
    "feed": {
      "host": "APPS_FEED_HOST",
      "port": {
        "__name": "APPS_FEED_PORT",
        "__format": "number"
      },
      "maxReleases": {
        "__name": "APPS_FEED_MAX_RELEASES",
        "__format": "number"
      }
    },
    "api": {
      "host": "APPS_API_HOST",
      "port": {
        "__name": "APPS_API_PORT",
        "__format": "number"
      },
      "basicAuth": {
        "username": "APPS_API_BASIC_AUTH_USERNAME",
        "password": "APPS_API_BASIC_AUTH_PASSWORD"
      }
    },
    "admin": {
      "host": "APPS_ADMIN_HOST",
      "port": {
        "__name": "APPS_ADMIN_PORT",
        "__format": "number"
      },
      "basicAuth": {
        "username": "APPS_ADMIN_BASIC_AUTH_USERNAME",
        "password": "APPS_ADMIN_BASIC_AUTH_PASSWORD"
      },
      "services": {
        "api": {
          "url": "APPS_ADMIN_SERVICES_API_URL",
          "basicAuth": {
            "username": "APPS_ADMIN_SERVICES_API_BASIC_AUTH_USERNAME",
            "password": "APPS_ADMIN_SERVICES_API_BASIC_AUTH_PASSWORD"
          }
        }
      }
    },
    "jobs": {
      "sync-releases": {
        "cron": {
          "pattern": "JOBS_SYNC_RELEASES_CRON_PATTERN",
          "timezone": "JOBS_SYNC_RELEASES_CRON_TIMEZONE",
          "tickerTimeout": {
            "__name": "JOBS_SYNC_RELEASES_CRON_TICKER_TIMEOUT",
            "__format": "number"
          }
        },
        "max-release-time": "JOBS_SYNC_RELEASES_MAX_RELEASE_TIME"
      }
    }
  },
  "database": {
    "path": "DATABASE_PATH"
  },
  "remote": {
    "itunes": {
      "search": {
        "url": "REMOTE_ITUNES_SEARCH_URL",
        "searchArtists": {
          "limit": {
            "__name": "REMOTE_ITUNES_SEARCH_SEARCH_ARTISTS_LIMIT",
            "__format": "number"
          }
        }
      },
      "lookup": {
        "url": "REMOTE_ITUNES_LOOKUP_URL",
        "getLatestsArtistMusicReleases": {
          "limit": {
            "__name": "REMOTE_ITUNES_LOOKUP_URL_GET_LATESTS_ARTIST_MUSIC_RELEASES_LIMIT",
            "__format": "number"
          }
        },
        "getLatestsArtistAlbumReleases": {
          "limit": {
            "__name": "REMOTE_ITUNES_LOOKUP_URL_GET_LATESTS_ARTIST_ALBUM_RELEASES_LIMIT",
            "__format": "number"
          }
        }
      }
    }
  },
  "media": {
    "placeholderImage": "MEDIA_PLACEHOLDER_IMAGE"
  }
}

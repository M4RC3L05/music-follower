{
  "apps": {
    "web": {
      "host": "APPS_WEB_HOST",
      "port": {
        "__name": "APPS_WEB_PORT",
        "__format": "number"
      },
      "basicAuth": {
        "username": "APPS_WEB_BASIC_AUTH_USERNAME",
        "password": "APPS_WEB_BASIC_AUTH_PASSWORD"
      },
      "feed": {
        "maxReleases": {
          "__name": "APPS_WEB_FEED_MAX_RELEASES",
          "__format": "number"
        }
      }
    },
    "jobs": {
      "sync-releases": {
        "max-release-time": "JOBS_SYNC_RELEASES_MAX_RELEASE_TIME"
      }
    }
  },
  "database": {
    "path": "DATABASE_PATH"
  },
  "remote": {
    "itunes": {
      "search": {
        "url": "REMOTE_ITUNES_SEARCH_URL",
        "searchArtists": {
          "limit": {
            "__name": "REMOTE_ITUNES_SEARCH_SEARCH_ARTISTS_LIMIT",
            "__format": "number"
          }
        }
      },
      "lookup": {
        "url": "REMOTE_ITUNES_LOOKUP_URL",
        "getLatestsArtistMusicReleases": {
          "limit": {
            "__name": "REMOTE_ITUNES_LOOKUP_URL_GET_LATESTS_ARTIST_MUSIC_RELEASES_LIMIT",
            "__format": "number"
          }
        },
        "getLatestsArtistAlbumReleases": {
          "limit": {
            "__name": "REMOTE_ITUNES_LOOKUP_URL_GET_LATESTS_ARTIST_ALBUM_RELEASES_LIMIT",
            "__format": "number"
          }
        }
      }
    }
  },
  "csrf": {
    "origin": "CSRF_ORIGIN"
  },
  "media": {
    "placeholderImage": "MEDIA_PLACEHOLDER_IMAGE"
  }
}
